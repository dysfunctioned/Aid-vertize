{
    "manifest_version": 3,
    "name": "Aid-vertize",
    "description": "Aid-vertize Beta/MVP browser extention",
    "version": "1.0.0",
    "action": {
        "default_popup": "popup.html",
        "default_title": "Aid-vertize",
        "default_icon": "icon.png"
    },
    "chrome_url_overrides": {
        "newtab": "newTab.html"
    },
    "permissions": [
        "tabs",
        "bookmarks",
        "storage",
        "identity",
        "history"
    ],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "options_page": "options.html",
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "content_security_policy": {
        "script_src": [
            "script-src 'self' https://pagead2.googlesyndication.com; object-src 'self';",
            "script-src-elem 'self' https://pagead2.googlesyndication.com;"
        ]
    }
}